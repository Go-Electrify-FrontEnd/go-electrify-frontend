# Example environment variables for E2E testing
# Copy this file to .env to run tests locally

# Authentication secret key for JWT token generation in tests
AUTH_SECRET_KEY=test-secret-key-for-testing-only-change-in-production

# Node environment
NODE_ENV=test

# Backend URL (mocked in tests, but required for build)
BACKEND_URL=http://localhost:8080/api/v1

# Font optimization - use fallback to avoid network calls in tests
NEXT_FONT_GOOGLE_FALLBACK=true
